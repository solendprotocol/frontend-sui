{
  "name": "@suilend/frontend-sui-local",
  "version": "0.1.53",
  "private": true,
  "description": "A collection of TypeScript frontend components and libraries used by Suilend and SpringSui",
  "author": "Suilend",
  "license": "MIT",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.js"
  },
  "types": "./src/index.ts",
  "scripts": {
    "build": "rm -rf ./dist && bun tsc",
    "eslint": "eslint --fix \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*\"",
    "lint": "bun eslint && bun prettier && bun tsc --noEmit",
    "release": "bun run build && bun ts-node ./release.ts && cd ./dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solendprotocol/frontend-sui.git"
  },
  "bugs": {
    "url": "https://github.com/solendprotocol/frontend-sui/issues"
  },
  "dependencies": {
    "@msafe/sui-app-store": "^0.0.161",
    "@msafe/sui-wallet": "^0.0.77",
    "@msafe/sui3-utils": "^5.0.2",
    "@mysten/dapp-kit": "0.14.33",
    "@mysten/sui": "1.14.3",
    "@mysten/wallet-standard": "0.13.12",
    "@sentry/nextjs": "^8.38.0",
    "@suilend/sdk": "^1.1.22",
    "@suilend/springsui-sdk": "^1.0.11",
    "@tanstack/react-query": "^5.60.2",
    "bignumber.js": "^9.1.2",
    "launchdarkly-react-client-sdk": "^3.6.0",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.56.0",
    "next": "^15.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-responsive": "^10.0.0",
    "shio-sdk": "^1.0.8",
    "sonner": "1.4.41",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.4",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@tsconfig/next": "^2.0.3",
    "@types/lodash": "^4.17.13",
    "@types/mixpanel-browser": "^2.50.2",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
